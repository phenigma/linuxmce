# /etc/fstab: static file system information.
#
# <file system> <mount point>   <type>  <options>               <dump>  <pass>
proc            /proc           proc    defaults                0       0
/dev/fd0        /floppy         auto    user,noauto             0       0
/dev/cdrom      /cdrom          iso9660 ro,user,noauto          0       0
sysfs		/sys		sysfs	defaults		0	0

%CORE_INTERNAL_ADDRESS%:/usr/pluto/diskless/%MOON_ID% 	/			nfs intr,udp,rsize=32768,wsize=32768,retrans=10,timeo=50 1 1
%CORE_INTERNAL_ADDRESS%:/usr/pluto/var/			/usr/pluto/var/  	nfs intr,udp,rsize=32768,wsize=32768,retrans=10,timeo=50 1 1
%CORE_INTERNAL_ADDRESS%:/usr/pluto/orbiter		/usr/pluto/orbiter 	nfs intr,udp,rsize=32768,wsize=32768,retrans=10,timeo=50 1 1
%CORE_INTERNAL_ADDRESS%:/usr/pluto/keys			/usr/pluto/keys 	nfs intr,udp,rsize=32768,wsize=32768,retrans=10,timeo=50 1 1
%CORE_INTERNAL_ADDRESS%:/usr/pluto/deb-cache		/usr/pluto/deb-cache	nfs intr,udp,rsize=32768,wsize=32768,retrans=10,timeo=50 1 1
