msgstr "IDE%s master (%s)"

#. Type: text
#. Description
#. For example IDE1 slave (hdd)
#: ../partman.templates:244
#, no-c-format
msgid "IDE%s slave (%s)"
msgstr "IDE%s slave (%s)"

#. Type: text
#. Description
#. For example IDE1 master, partition #5 (hdc5)
#: ../partman.templates:249
#, no-c-format
msgid "IDE%s master, partition #%s (%s)"
msgstr "IDE%s master, پارتیشن #%s (%s)"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5 (hdf5)
#: ../partman.templates:254
#, no-c-format
msgid "IDE%s slave, partition #%s (%s)"
msgstr "IDE%s slave, پارتیشن  #%s (%s)"

#. Type: text
#. Description
#: ../partman.templates:258
#, no-c-format
msgid "SCSI%s (%s,%s,%s) (%s)"
msgstr "SCSI%s (%s,%s,%s) (%s)"

#. Type: text
#. Description
#: ../partman.templates:262
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s (%s)"
msgstr "SCSI%s (%s,%s,%s), پارتیشن #%s (%s)"

#. Type: text
#. Description
#: ../partman.templates:266
#, no-c-format
msgid "RAID%s device #%s"
msgstr "RAID%s دستگاه #%s"

